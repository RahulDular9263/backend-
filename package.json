{
    "name": "my-app",
    "version": "0.1.0",
    "description": "My React Desktop App",
    "author": "Your Name",
    "private": true,
    "main": "main.js",
    "dependencies": {
        "axios": "^1.5.0",
        "cors": "^2.8.5",
        "express": "^5.1.0",
        "fs": "^0.0.1-security",
        "multer": "^1.4.5-lts.2",
        "mysql2": "^3.14.1",
        "path": "^0.12.7",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.21.1",
        "react-scripts": "5.0.1"
    },
    "devDependencies": {
        "concurrently": "^9.1.2",
        "electron": "^35.2.1",
        "electron-builder": "^26.0.12",
        "wait-on": "^8.0.3"
    },
    "scripts": {
        "start": "concurrently \"npm run react-start\" \"npm run electron\"\"node server.js\"",
        "react-start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "electron": "wait-on http://localhost:3000 && electron .",
        "dist": "npm run build && electron-builder"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "build": {
        "appId": "com.myapp.id",
        "productName": "MyReactApp",
        "files": [
            "build/**/*",
            "main.js"
        ],
        "directories": {
            "buildResources": "assets"
        },
        "win": {
            "icon": "assets/icon.ico",
            "target": "nsis"
        }
    }
}
